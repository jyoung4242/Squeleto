{  
 "compilerOptions": { 
   "target": "ESNext",
   "useDefineForClassFields": true,
   "lib": ["ESNext", "DOM"],
   "outDir": "./build/",
   "sourceMap": true, 
   "noImplicitAny": true,
   "module": "ESNext",
   "jsx": "react",
   "allowJs": true,
   "moduleResolution": "Node",
   "resolveJsonModule": true,
   "esModuleInterop": true,
   "skipLibCheck": true,
   "isolatedModules": true,
   "strict": true
   }
 }
